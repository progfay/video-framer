{
  "name": "Video Framer",
  "short_name": "Video Framer",
  "start_url": "/video-framer/index.html",
  "icons": [
    {
      "src": "/video-framer/images/android-chrome-192x192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "/video-framer/images/android-chrome-512x512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "share_target": {
    "action": "/video-framer/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "files": [
        {
          "name": "file",
          "accept": ["video/*"]
        }
      ]
    }
  },
  "theme_color": "#dddddd",
  "background_color": "#dddddd",
  "display": "standalone",
  "orientation": "portrait"
}
